{
    "name": "lowerrocklabs/laravel-livewire-tables-advanced-filters",
    "description": "Advanced filters for Rappasoft Laravel Livewire Tables using FlatPickr",
    "keywords": [
        "lowerrocklabs",
        "livewire",
        "datatables",
        "tables",
        "laravel"
    ],
    "homepage": "https://github.com/LowerRockLabs/LaravelLivewireTablesAdvancedFilters",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe",
            "email": "joe@lowerrocklabs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^6.13|^7.0|^8.0",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "laravel/pint": "^1.2",
        "nunomaduro/larastan": "^2.2",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "LowerRockLabs\\LaravelLivewireTablesAdvancedFilters\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LowerRockLabs\\LaravelLivewireTablesAdvancedFilters\\LaravelLivewireTablesAdvancedFiltersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}